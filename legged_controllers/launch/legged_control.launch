<?xml version="1.0" ?>

<launch>
    <include file="$(find legged_controllers)/launch/load_controller.launch"/>
    
    <node pkg="ocs2_legged_robot_ros" type="legged_robot_gait_command" name="legged_robot_gait_command"
            output="screen" launch-prefix="tmux new-window -d"/>

    <node pkg="ocs2_legged_robot_ros" type="legged_robot_target" name="legged_robot_target"
        output="screen" launch-prefix="tmux new-window -d"/>

    <node pkg="legged_controllers" type="legged_target_trajectories_publisher" name="legged_target_trajectories_publisher"
        output="screen"/>
            
</launch>
